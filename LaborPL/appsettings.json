{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "LaborBLL": "Debug",
      "LaborDAL": "Debug"
    }
  },
  "Serilog": {
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "File", "Args": { "path": "logs/log-.txt", "rollingInterval": "Day" } }
    ]
  },
  "ConnectionStrings": {
  "DefaultConnection": "Server=.;Database=LaborMVC;Integrated Security=SSPI;TrustServerCertificate=True"
},
  "AllowedHosts": "*"
}
